{
  "entryPoints": ["src/index.ts"],
  "tsconfig": "tsconfig.json",
  "plugin": ["typedoc-plugin-markdown"],
  "out": "docs/api",
  "readme": "none",

  "excludePrivate": true,
  "excludeInternal": true,

  "hideGenerator": true,
  "categorizeByGroup": true,
  "cleanOutputDir": true,

  "sort": ["visibility", "source-order"]
}